#include <stdio.h>

int main (void){
    
    int num1, num2,soma;

    printf ("Insira o valor do primeiro número: ");
    scanf ("%d" , &num1);

    printf ("Insira o valor do segundo número: ");
    scanf ("%d" , &num2);
   
    soma = num1+num2;

    printf ("Resultado da soma é: %d\n", soma);

    return 0;
}
